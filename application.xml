<application xmlns="http://ns.adobe.com/air/application/3.3">
  <id>com.battybovine.BatchFlash</id>
  <versionNumber>0.5.5</versionNumber>
  <filename>BatchFlash</filename>
  <name>BatchFlash</name>
  <description>Automatically render multiple SWF video files to an image sequence in the given folder.</description>
  <copyright>©2012 Batty Bovine Productions, LLC. All Rights Reserved.</copyright>
  <initialWindow>
    <title>BatchFlash</title>
    <content>BatchFlash.swf</content>
    <visible>true</visible>
    <minimizable>true</minimizable>
    <maximizable>true</maximizable>
    <systemChrome>standard</systemChrome>
  </initialWindow>
  <!--
	More options:
	http://livedocs.adobe.com/flex/3/html/File_formats_1.html#1043413
	-->
  <allowBrowserInvocation>false</allowBrowserInvocation>
  <supportedProfiles>desktop extendedDesktop</supportedProfiles>
  <icon>
    <image16x16>icons/BatchFlash16.png</image16x16>
    <image32x32>icons/BatchFlash32.png</image32x32>
    <image48x48>icons/BatchFlash48.png</image48x48>
    <image128x128>icons/BatchFlash128.png</image128x128>
    <image512x512>icons/BatchFlash512.png</image512x512>
  </icon>
</application>